<div class="welcome">
  <div class="logo">
    <%= image_tag("localhost-logo-black.svg", alt: "Localhost Logo", width: '500') %>
  </div>

<ul class="social">
  <li>
    <a href="https://www.meetup.com/web-industry-meetup/" target="_blank" class="social" title="Follow Localhost on Meetup"><%= image_tag("social/meetup-logo.jpeg", alt: "Meetup") %><span class="sr-only">Follow Localhost on Meetup</span></a>
  </li>
  <li>
    <a href="https://twitter.com/localhostau" target="_blank" class="social" title="Follow Localhost on Twitter"><%= image_tag("social/Twitter_Logo_Blue.svg", alt: "Twitter") %><span class="sr-only">Follow Localhost on Twitter</span></a>
  </li>
</ul>

<ol>
  <% data.otherevents.events.each do |event| %>
  <li><%= event.link %>  <%= event.date %> - <%= event.title %></li>
  <% end %>
</ol>

